{
  "name": "transaction-page",
  "version": "1.0.0",
  "description": "transaction page",
  "main": "dist/server.js",
  "author": "Mohamed Tharwat",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:watch": "onchange src -- npm run build",
    "start": "node dist/backend/server.js",
    "start:watch": "nodemon dist/backend/server.js",
    "lint": "eslint --fix src",
    "lint:watch": "onchange -k src -- npm run lint ",
    "format": "prettier --write src",
    "format:watch": "onchange -k src -- npm run format ",
    "format:lint": "onchange -k src -- npm-run-all format lint",
    "build:start": "onchange -k src -- npm-run-all build start",
    "all": "onchange -k src -- npm-run-all format lint build start"
  },
  "dependencies": {
    "@types/node": "^16.18.89",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "express": "^4.18.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prettier": "3.2.5",
    "typescript": "^4.9.5"
  }
}
